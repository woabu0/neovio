<launch>
  <arg name="scan_topic" default="scan" />
  <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
    <rosparam>
      map_update_interval: 5.0
      maxUrange: 6.0
      maxRange: 8.0
      sigma: 0.05
      kernelSize: 1
      lstep: 0.05
      astep: 0.05
      iterations: 5
      lsigma: 0.075
      ogain: 3.0
      lskip: 0
      srr: 0.1
      srt: 0.2
      str: 0.1
      stt: 0.2
      linearUpdate: 1.0
      angularUpdate: 0.5
      temporalUpdate: -1.0
      resampleThreshold: 0.5
      particles: 30
      xmin: -50.0
      ymin: -50.0
      xmax: 50.0
      ymax: 50.0
      delta: 0.05
      occ_thresh: 0.25
      llsamplerange: 0.01
      llsamplestep: 0.01
      lasamplerange: 0.005
      lasamplestep: 0.005
    </rosparam>
  </node>
</launch>
